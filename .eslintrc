{
    "extends": "himynameisdave/configurations/browser",
    "rules": {
        "arrow-body-style": 0,
        "arrow-parens": [2, "as-needed"],
        "no-invalid-this": 0,
        "react/jsx-indent-props": [2, 4],
        "react/jsx-max-props-per-line": [2, { "maximum": 3 }],
        "react/jsx-sort-props": [2, {
            "callbacksLast": true,
            "reservedFirst": true,
            "shorthandLast": true
        }]
    }
}
